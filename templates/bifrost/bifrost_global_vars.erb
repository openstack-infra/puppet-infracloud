node_network_info: "{}"
inventory_dhcp: true
ipv4_subnet_mask: 255.255.255.192
ipv4_gateway: 10.10.16.131
ipv4_nameserver: 8.8.8.8
dhcp_pool_start: 10.10.16.144
dhcp_pool_end: 10.10.16.190
dib_os_element: "ubuntu-minimal"
